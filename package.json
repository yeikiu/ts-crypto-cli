{
  "name": "ts-kraken-api",
  "version": "1.0.4",
  "description": "TS kraken.com API client library for NodeJS",
  "keywords": [
    "kraken",
    "api",
    "btc",
    "bitcoin",
    "ts",
    "typescript"
  ],
  "author": "JQ (https://github.com/yeikiu)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/yeikiu/ts-kraken-api.git"
  },
  "bugs": {
    "url": "https://github.com/yeikiu/ts-kraken-api/issues"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "axios": "^0.19.2",
    "debug": "^4.1.1",
    "qs": "^6.9.1",
    "setup-gh-badges": "^0.0.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^13.5.0",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "build": "tsc -p .",
    "start": "    yarn build &&     DEBUG=ts-kraken-api:*        node    dist/index.js",
    "start:win": "yarn build && set DEBUG=ts-kraken-api:* &&     node    dist/index.js",
    "watch": "                      DEBUG=ts-kraken-api:*        nodemon src/index.ts",
    "watch:win": "              set DEBUG=ts-kraken-api:* &&     nodemon src/index.ts",
    "badges:win": "./scripts/generate_badges_win.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./scripts/generate_badges_win.sh"
    }
  }
}
