    "kraken",
    "api",
    "btc",
    "bitcoin",
    "ts",
    "typescript"
  ],
  "author": "JQ (https://github.com/yeikiu)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/yeikiu/ts-kraken-api.git"
  },
  "bugs": {
    "url": "https://github.com/yeikiu/ts-kraken-api/issues"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "axios": "^0.19.2",
    "debug": "^4.1.1",
    "qs": "^6.9.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-tsdoc": "^0.2.1",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "^8.6.2",
    "typedoc": "^0.16.9",
    "typescript": "^3.7.5"
  },
  "resolutions": {
    "minimist": "^1.2.3",
    "acorn": "^7.1.1"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p .",
