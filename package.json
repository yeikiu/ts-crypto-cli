{
  "name": "ts-crypto-cli",
  "version": "0.6.0",
  "description": "A handy npm cli to operate against some of the most known Crypto Exchanges",
  "keywords": [
    "kraken",
    "api",
    "btc",
    "bitcoin",
    "ts",
    "typescript"
  ],
  "author": "JQ (https://github.com/yeikiu)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/yeikiu/ts-crypto-cli.git"
  },
  "bugs": {
    "url": "https://github.com/yeikiu/ts-crypto-cli/issues"
  },
  "license": "CC0-1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "dist/cli.js",
  "dependencies": {
    "axios": "^0.19.2",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "moment": "^2.27.0",
    "node-menu": "^1.3.0",
    "qs": "^6.9.1",
    "rotating-file-stream": "^2.1.1",
    "rxjs": "^6.5.5",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "@types/node": "11.11.0",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-tsdoc": "^0.2.1",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^8.6.2",
    "typedoc": "^0.16.9",
    "typescript": "^4.0.2"
  },
  "resolutions": {
    "minimist": "^1.2.3",
    "acorn": "^7.1.1",
    "lodash": "4.17.19"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "eslint-plugin-tsdoc"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:import/typescript"
    ],
    "rules": {
      "tsdoc/syntax": "warn"
    }
  },
  "scripts": {
    "build": "rimraf dist && tsc -p .",
    "lint": "tsc -p . --noEmit && eslint \"src/**/*.{js,ts}\" --quiet --fix",
    "start": "npm run build && node dist/cli.js",
    "watch": "nodemon",
    "audit": "npx yarn audit"
  }
}
